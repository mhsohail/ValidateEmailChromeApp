{
    "manifest_version": 2,

    "name": "Verify Email",
    "description": "This extension verifies email.",
    "version": "1.0",

    "permissions": [
        "http://*/*",
		"https://*/*"
    ],
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html",
        "default_title": "Verify Email"
    }
}