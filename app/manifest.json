{
	// Required
    "manifest_version": 2,
    "name": "Verify Email",
    "version": "1.1.1",
	
	// Recommended
    "description": "This extension verifies email.",
	
	"permissions": [
		"tabs",
        "http://*/*",
		"https://*/*"
    ],
	
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html",
        "default_title": "Verify Email"
    }
}